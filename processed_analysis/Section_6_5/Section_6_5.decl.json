p [a.vanish n (by grind)]","original":true},"params":[{"type":[935,943],"ref":[933,934],"id":[933,934],"bi":"default"}],"name":["Chapter6","Sequence","pow_one"],"modifiers":null,"kind":"theorem","id":{"range":[915,931],"original":true}},{"value":{"range":[1126,1228],"original":true},"type":{"range":[1108,1125],"original":true},"signature":{"range":{"stop":1125,"start":1085},"pp":" (a : Sequence) (k : ℕ) : a ^ (k + 1) = a ^ k * a","original":true},"scopeInfo":{"varDecls":[],"scopedOpenDecl":[["Chapter6","Sequence"],["Chapter6"]],"openDecl":[],"omitVars":[],"levelNames":[],"includeVars":[],"currNamespace":["Chapter6","Sequence"]},"ref":{"range":{"stop":1228,"start":1061},"pp":"theorem pow_succ (a : Sequence) (k : ℕ) : a ^ (k + 1) = a ^ k * a := by\n  ext <;> simp only [HPow.hPow, Pow.pow, HMul.hMul, Mul.mul, max_self]; split_ifs with h <;> rfl","original":false},"params":[{"type":[1088,1096],"ref":[1086,1087],"id":[1086,1087],"bi":"default"},{"type":[1101,1104],"ref":[1099,1100],"id":[1099,1100],"bi":"default"}],"name":["Chapter6","Sequence","pow_succ"],"modifiers":null,"kind":"theorem","id":{"range":[1067,1084],"original":true}},{"value":{"range":[1370,2278],"original":true},"type":{"range":[1303,1369],"original":true},"signature":{"range":{"stop":1369,"start":1289},"pp":" {k : ℕ} : ((fun (n : ℕ) ↦ 1 / ((n : ℝ) + 1) ^ (1 / (k + 1 : ℝ))) : Sequence).TendsTo 0","original":true},"scopeInfo":{"varDecls":[],"scopedOpenDecl":[["Chapter6","Sequence"],["Chapter6"]],"openDecl":[],"omitVars":[],"levelNames":[],"includeVars":[],"currNamespace":["Chapter6","Sequence"]},"ref":{"range":{"stop":2278,"start":1230},"pp":"/-- Corollary 6.5.1 -/\ntheorem lim_of_power_decay {k : ℕ} : ((fun (n : ℕ) ↦ 1 / ((n : ℝ) + 1) ^ (1 / (k + 1 : ℝ))) : Sequence).TendsTo 0 := by\n  -- This proof is written to follow the structure of the original text.\n  \n  set a := ((fun (n : ℕ) ↦ 1 / ((n : ℝ) + 1) ^ (1 / (k + 1 : ℝ))) : Sequence)\n  have ha : a.BddBelow := by use 0; intro n _; simp [a]; positivity\n  have ha' : a.IsAntitone := by\n    intro n hn; observe hn' : 0 ≤ n + 1; simp [a, hn, hn']\n    rw [inv_le_inv₀, Real.rpow_le_rpow_iff] <;> try positivity\n    simp\n  apply convergent_of_antitone ha at ha'\n  have hpow (n : ℕ) : (a ^ (n + 1)).Convergent ∧ lim (a ^ (n + 1)) = (lim a) ^ (n + 1) :=\n    by\n    induction' n with n ih\n    . simp [ha', -dite_pow]\n    rw [pow_succ]; convert lim_mul ih.1 ha'; grind\n  have hlim : (lim a) ^ (k + 1) = 0 := by\n    rw [← (hpow k).2]; convert lim_harmonic.2; ext; rfl\n    simp only [HPow.hPow, Pow.pow, a]; split_ifs with h <;> simp\n    rw [← Real.rpow_natCast, ← Real.rpow_mul (by positivity)]\n    convert Real.rpow_one _; field_simp\n  simp [lim_eq, ha', pow_eq_zero hlim]","original":true},"params":[{"type":[1292,1295],"ref":[1290,1291],"id":[1290,1291],"bi":"implicit"}],"name":["Chapter6","Sequence","lim_of_power_decay"],"modifiers":null,"kind":"theorem","id":{"range":[1261,1288],"original":true}},{"value":{"range":[2417,2430],"original":true},"type":{"range":[2374,2416],"original":true},"signature":{"range":{"stop":2416,"start":2350},"pp":" {x : ℝ} (hx : |x| < 1) : ((fun (n : ℕ) ↦ x ^ n) : Sequence).TendsTo 0","original":true},"scopeInfo":{"varDecls":[],"scopedOpenDecl":[["Chapter6","Sequence"],["Chapter6"]],"openDecl":[],"omitVars":[],"levelNames":[],"includeVars":[],"currNamespace":["Chapter6","Sequence"]},"ref":{"range":{"stop":2430,"start":2280},"pp":"/-- Lemma 6.5.2 / Exercise 6.5.2 -/\ntheorem lim_of_geometric {x : ℝ} (hx : |x| < 1) : ((fun (n : ℕ) ↦ x ^ n) : Sequence).TendsTo 0 := by sorry","original":true},"params":[{"type":[2353,2356],"ref":[2351,2352],"id":[2351,2352],"bi":"implicit"},{"type":[2363,2370],"ref":[2359,2361],"id":[2359,2361],"bi":"default"}],"name":["Chapter6","Sequence","lim_of_geometric"],"modifiers":null,"kind":"theorem","id":{"range":[2324,2349],"original":true}},{"value":{"range":[2568,2581],"original":true},"type":{"range":[2525,2567],"original":true},"signature":{"range":{"stop":2567,"st