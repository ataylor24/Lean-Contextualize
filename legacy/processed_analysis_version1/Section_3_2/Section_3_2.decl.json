","pair_exists"],"modifiers":null,"kind":"theorem","id":{"range":[2836,2861],"original":true}},{"value":{"range":[3281,3294],"original":true},"type":{"range":[3229,3280],"original":true},"signature":{"range":[3176,3280],"pp":" (h : axiom_of_universal_specification) (A B : Set) : ∃ (Z : Set), ∀ z, z ∈ Z ↔ z ∈ A ∨ z ∈ B","original":true},"scopeInfo":{"varDecls":["variable [SetTheory]"],"scopedOpenDecl":[["Chapter3","SetTheory"],["Chapter3"],["Chapter3","SetTheory","Set"]],"openDecl":[],"omitVars":[],"levelNames":[],"includeVars":[],"currNamespace":["Chapter3","SetTheory","Set"]},"ref":{"range":[2993,3294],"pp":"/-- Exercise 3.2.1. The spirit of the exercise is to establish these results without using either\n  Russell's paradox, or the union operation.\n-/\ntheorem union_exists (h : axiom_of_universal_specification) (A B : Set) : ∃ (Z : Set), ∀ z, z ∈ Z ↔ z ∈ A ∨ z ∈ B := by\n  sorry","original":true},"params":[{"type":[3180,3212],"ref":[3177,3178],"id":[3177,3178],"bi":"default"},{"type":[3219,3222],"ref":[3215,3216],"id":[3215,3216],"bi":"default"},{"type":[3219,3222],"ref":[3217,3218],"id":[3217,3218],"bi":"default"}],"name":["Chapter3","SetTheory","Set","union_exists"],"modifiers":null,"kind":"theorem","id":{"range":[3149,3175],"original":true}},{"value":{"range":[3614,3627],"original":true},"type":{"range":[3550,3613],"original":true},"signature":{"range":[3483,3613],"pp":" (h : axiom_of_universal_specification) (A : Set) (P : A → Prop) : ∃ (Z : Set), ∀ z, z ∈ Z ↔ ∃ h : z ∈ A, P ⟨z, h⟩","original":true},"scopeInfo":{"varDecls":["variable [SetTheory]"],"scopedOpenDecl":[["Chapter3","SetTheory"],["Chapter3"],["Chapter3","SetTheory","Set"]],"openDecl":[],"omitVars":[],"levelNames":[],"includeVars":[],"currNamespace":["Chapter3","SetTheory","Set"]},"ref":{"range":[3296,3627],"pp":"/-- Exercise 3.2.1. The spirit of the exercise is to establish these results without using either\n  Russell's paradox, or the specify operation.\n-/\ntheorem specify_exists (h : axiom_of_universal_specification) (A : Set) (P : A → Prop) :\n    ∃ (Z : Set), ∀ z, z ∈ Z ↔ ∃ h : z ∈ A, P ⟨z, h⟩ := by sorry","original":true},"params":[{"type":[3487,3519],"ref":[3484,3485],"id":[3484,3485],"bi":"default"},{"type":[3524,3527],"ref":[3522,3523],"id":[3522,3523],"bi":"default"},{"type":[3533,3543],"ref":[3530,3531],"id":[3530,3531],"bi":"default"}],"name":["Chapter3","SetTheory","Set","specify_exists"],"modifiers":null,"kind":"theorem","id":{"range":[3454,3482],"original":true}},{"value":{"range":[3992,4005],"original":true},"type":{"range":[3943,3991],"original":true},"signature":{"range":[3816,3991],"pp":" (h : axiom_of_universal_specification) (A : Set) (P : A → Object → Prop) (hP : ∀ x y y', P x y ∧ P x y' → y = y') :\n  ∃ (Z : Set), ∀ y, y ∈ Z ↔ ∃ a : A, P a y","original":true},"scopeInfo":{"varDecls":["variable [SetTheory]"],"scopedOpenDecl":[["Chapter3","SetTheory"],["Chapter3"],["Chapter3","SetTheory","Set"]],"openDecl":[],"omitVars":[],"levelNames":[],"includeVars":[],"currNamespace":["Chapter3","SetTheory","Set"]},"ref":{"range":[3629,4005],"pp":"/-- Exercise 3.2.1. The spirit of the exercise is to establish these results without using either\n  Russell's paradox, or the replace operation.\n-/\ntheorem replace_exists (h : axiom_of_universal_specification) (A : Set) (P : A → Object → Prop)\n    (hP : ∀ x y y', P x y ∧ P x y' → y = y') : ∃ (Z : Set), ∀ y, y ∈ Z ↔ ∃ a : A, P a y := by sorry","original":true},"params":[{"type":[3820,3852],"ref":[3817,3818],"id":[3817,3818],"bi":"default"},{"type":[3857,3860],"ref":[3855,3856],"id":[3855,3856],"bi":"default"},{"type":[3868,3889],"ref":[3865,3866],"id":[3865,3866],"bi":"default"},{"type":[3896,3935],"ref":[3892,3894],"id":[3892,3894],"bi":"default"}],"name":["Chapter3","SetTheory","Set","replace_exists"],"modifiers":null,"kind":"theorem","id":{"range":[3787,3815],"original":true}},{"value":{"range":[4091,4102],"original":true},"type":{"range":[4074,4090],"original":true},"signature":{"range":[4064,4090],"