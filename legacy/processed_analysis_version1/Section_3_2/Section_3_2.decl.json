[{"value":{"range":[1197,1271],"original":true},"type":{"range":[1192,1196],"original":true},"signature":{"range":[1190,1196],"pp":" : Prop","original":true},"scopeInfo":{"varDecls":["variable [SetTheory]"],"scopedOpenDecl":[["Chapter3"]],"openDecl":[],"omitVars":[],"levelNames":[],"includeVars":[],"currNamespace":["Chapter3"]},"ref":{"range":[1107,1271],"pp":"/-- Axiom 3.8 (Universal specification) -/\nabbrev axiom_of_universal_specification : Prop :=\n  ∀ P : Object → Prop, ∃ A : Set, ∀ x : Object, x ∈ A ↔ P x","original":true},"params":[],"name":["Chapter3","axiom_of_universal_specification"],"modifiers":null,"kind":"abbrev","id":{"range":[1157,1189],"original":true}},{"value":{"range":[1336,1776],"original":true},"type":{"range":[1300,1335],"original":true},"signature":{"range":[1298,1335],"pp":" : ¬axiom_of_universal_specification","original":true},"scopeInfo":{"varDecls":["variable [SetTheory]"],"scopedOpenDecl":[["Chapter3"]],"openDecl":[],"omitVars":[],"levelNames":[],"includeVars":[],"currNamespace":["Chapter3"]},"ref":{"range":[1273,1776],"pp":"theorem Russells_paradox : ¬axiom_of_universal_specification := by\n  -- This proof is written to follow the structure of the original text.\n  \n  intro h\n  set P : Object → Prop := fun x ↦ ∃ X : Set, x = X ∧ x ∉ X\n  choose Ω hΩ using h P\n  by_cases h : (Ω : Object) ∈ Ω\n  . have : P (Ω : Object) := (hΩ _).mp h\n    obtain ⟨Ω', ⟨hΩ1, hΩ2⟩⟩ := this\n    simp at hΩ1\n    rw [← hΩ1] at hΩ2\n    contradiction\n  have : P (Ω : Object) := by use Ω\n  rw [← hΩ] at this\n  contradiction","original":true},"params":[],"name":["Chapter3","Russells_paradox"],"modifiers":null,"kind":"theorem","id":{"range":[1281,1297],"original":true}},{"value":{"range":[1926,2124],"original":true},"type":{"range":[1879,1925],"original":true},"signature":{"range":[1850,1925],"pp":" {A : Set} (h : A ≠ ∅) : ∃ x : A, ∀ S : Set, x.val = S → Disjoint S A","original":true},"scopeInfo":{"varDecls":["variable [SetTheory]"],"scopedOpenDecl":[["Chapter3","SetTheory"],["Chapter3"],["Chapter3","SetTheory","Set"]],"openDecl":[],"omitVars":[],"levelNames":[],"includeVars":[],"currNamespace":["Chapter3","SetTheory","Set"]},"ref":{"range":[1778,2124],"pp":"/-- Axiom 3.9 (Regularity) -/\ntheorem axiom_of_regularity {A : Set} (h : A ≠ ∅) : ∃ x : A, ∀ S : Set, x.val = S → Disjoint S A :=\n  by\n  choose x h h' using regularity_axiom A (nonempty_def h)\n  use ⟨x, h⟩\n  intro S hS; specialize h' S hS\n  rw [disjoint_iff, eq_empty_iff_forall_notMem]\n  contrapose! h'; simp at h'\n  aesop","original":true},"params":[{"type":[1853,1856],"ref":[1851,1852],"id":[1851,1852],"bi":"implicit"},{"type":[1862,1871],"ref":[1859,1860],"id":[1859,1860],"bi":"default"}],"name":["Chapter3","SetTheory","Set","axiom_of_regularity"],"modifiers":null,"kind":"theorem","id":{"range":[1816,1849],"original":true}},{"value":{"range":[2378,2391],"original":true},"type":{"range":[2350,2377],"original":true},"signature":{"range":[2307,2377],"pp":" (h : axiom_of_universal_specification) : ∃ (X : Set), ∀ x, x ∉ X","original":true},"scopeInfo":{"varDecls":["variable [SetTheory]"],"scopedOpenDecl":[["Chapter3","SetTheory"],["Chapter3"],["Chapter3","SetTheory","Set"]],"openDecl":[],"omitVars":[],"levelNames":[],"includeVars":[],"currNamespace":["Chapter3","SetTheory","Set"]},"ref":{"range":[2126,2391],"pp":"/-- Exercise 3.2.1.  The spirit of the exercise is to establish these results without using either\n  Russell's paradox, or the empty set.\n-/\ntheorem emptyset_exists (h : axiom_of_universal_specification) : ∃ (X : Set), ∀ x, x ∉ X := by sorry","original":true},"params":[{"type":[2311,2343],"ref":[2308,2309],"id":[2308,2309],"bi":"default"}],"name":["Chapter3","SetTheory","Set","emptyset_exists"],"modifiers":null,"kind":"theorem","id":{"range":[2277,2306],"original":true}},{"value":{"range":[2671,2684],"original":true},"type":{"range":[2633,2670],"original":true},"signature":{"range":[2579,2670],"pp":" (h : axiom_of_universal_specification) (x : Objec